{
    // 监听端口
    "localaddr": ":2081",
    // 连接目标，含端口
    "remoteaddr": "KCP_SERVER",
    // KCP密码
    "key": "KCP_PWD",
    // 加密方式
    "crypt": "KCP_CR",
    // 加速模式
    "mode": "KCP_MODE",
    // 并行连接数
    "conn": KCP_CONN,
    // 单个连接的自动过期时间
    "autoexpire": KCP_KTIME,
    // 过期连接的存活时间
    "scavengettl": KCP_TTIME,
    // MTU
    "mtu": KCP_MTU,
    // 发送窗口大小
    "sndwnd": KCP_SW,
    // 接收窗口大小
    "rcvwnd": KCP_RW,
    // 数据包
    "datashard": KCP_DS,
    // 校验包
    "parityshard": KCP_PS,
    // DSCP
    "dscp": KCP_SDSCP,
    // 每个接口的缓冲大小
    "sockbuf": KCP_BUF,
    // 禁用压缩
    "nocomp": KCP_NCP,
    // 心跳时间，秒
    "keepalive": KCP_KAL,
    // 模拟TCP连接
    "tcp": KCP_TCP,
    // 日志文件路径
    "log": "/root/hmss/kcpc.log",
    // 静默模式
    "quiet": true
}